<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="YoyoDefaultDiskEntry" parent="AdwActionRow">
    <property name="title">/dev/unit</property>
    <property name="subtitle">-1 GB</property>
    <child type="prefix">
      <object class="GtkCheckButton" id="chk_button">
        <property name="valign">center</property>
      </object>
    </child>
    <property name="activatable-widget">chk_button</property>
  </template>
</interface>
